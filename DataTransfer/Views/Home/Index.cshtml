
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<ul style="list-style: none">
    <li>@ViewBag.name</li>
    <li>@ViewData["surname"]</li>
    <li>@TempData["age"]</li>
</ul>
<hr />
<ul style="list-style: none; color: red">
    @foreach (var item in ViewBag.students)
   {
       <li>@item.Name @item.Surname @item.Id</li>
   }
</ul>

